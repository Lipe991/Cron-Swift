import Foundation


internal enum FieldPattern {
    case Any                 // *
    case Range(Int, Int)     // -
    case LastDayOfMonth      // L
    case LastWeekdayOfMonth  // LW
    case Weekday(Int)        // W
    case NthDayOfWeek(Int)   // #
    indirect case Repeat(FieldPattern, Int)       // /
    indirect case Or(FieldPattern, FieldPattern)  // ,
}

